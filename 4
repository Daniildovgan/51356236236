#include <iostream>
#include <limits>

int main() {
    const int months = 12;
    double profit[months];
    
    for (int i = 0; i < months; ++i) {
        std::cin >> profit[i];
    }
    
    double max_profit = std::numeric_limits<double>::min();
    double min_profit = std::numeric_limits<double>::max();
    int max_month, min_month;
    
    for (int i = 0; i < months; ++i) {
        if (profit[i] > max_profit) {
            max_profit = profit[i];
            max_month = i + 1;
        }
        if (profit[i] < min_profit) {
            min_profit = profit[i];
            min_month = i + 1;
        }
    }
    
    std::cout << max_month << " " << min_month << std::endl;
    
    return 0;
}

#include <iostream>
#include <cstdlib>
#include <ctime>
#include <limits>

int main() {
    const int size = 10;
    int arr[size];
    
    srand(time(nullptr));
    for (int i = 0; i < size; ++i) {
        arr[i] = rand() % 100;
    }
    
    int min_element = std::numeric_limits<int>::max();
    int max_element = std::numeric_limits<int>::min();
    
    for (int i = 0; i < size; ++i) {
        if (arr[i] < min_element) {
            min_element = arr[i];
        }
        if (arr[i] > max_element) {
            max_element = arr[i];
        }
    }
    
    std::cout << min_element << " " << max_element << std::endl;
    
    return 0;
}
